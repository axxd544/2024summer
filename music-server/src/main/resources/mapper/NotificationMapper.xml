<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.liu.mapper.NotificationMapper">
    <select id="getLatestNotification" resultType="com.example.liu.model.domain.Notification">
        SELECT * FROM notifications ORDER BY id DESC LIMIT 1
    </select>
</mapper>